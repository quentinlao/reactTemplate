{
    "compilerOptions": {
        "esModuleInterop": true,
        "jsx": "react",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": ["dom", "esnext"],
        "strict": true,
        "sourceMap": true,
        "target": "esnext",
        "plugins": [{ "name": "typescript-plugin-css-modules" }]
    },
    "exclude": ["node_modules"]
}
